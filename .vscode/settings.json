{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true
  },
  "workspace.isHidden": true,
  "files.associations": {
    "*.{setting,comp,fuse,fu,scriptlib}": "lua",
    "*.json5": "json5",
    "*.json": "jsonc"
  },
  "svelte.plugin.svelte.compilerWarnings": {
    "a11y-click-events-have-key-events": "ignore"
  }
}